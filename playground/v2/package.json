{
  "name": "mophidian",
  "version": "1.0.0",
  "scripts": {
    "css": "sass styles:static/css",
    "css:watch": "sass --watch styles:static/css",
    "css:compress": "sass --style=compressed styles:static/css",
    "css:watch:compress": "sass --watch --style=compressed styles:static/css",
    "tailwind": "tailwindcss -i ./styles/tailwind.css -o ./site/css/tailwind.css",
    "tailwind:mini": "tailwindcss -i ./styles/tailwind.css -o ./static/css/tailwind.min.css --minify",
    "tailwind:watch": "tailwindcss -i ./styles/tailwind.css -o ./static/css/tailwind.css --watch"
  },
  "dependencies": {
    "sass": "^1.55.0",
    "scss": "^0.2.4"
  },
  "devDependencies": {
    "tailwindcss": "^3.1.8"
  }
}
