<ul @if="not blank(nav)" #nav :id="'nav-'+nav.name">
    <li @for="item in nav" :class="'li-indent' if isinstance(item, mophidian.Nav) else ''">
        <a @if="isinstance(item, mophidian.Renderable) and item.url != url('')" :href="item.url">{item.title}</a>
        <nav @elif="isinstance(item, mophidian.Nav)" :nav="item" />
    </li>
</ul>