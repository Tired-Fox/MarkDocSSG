<div class="header">
    {% block header_logo %}<h5>{{ global.header_logo }}</h5>{% endblock %}
    {% block header_nav %}
    <nav>
        <ul>
            {% for link in header_nav %}
                <li class="nav-{{link[0]}}">
                    <a href="{{link[1]}}">
                        {{link[0]}}
                    </a>
                </li>
            {% endfor %}
        </ul>
    </nav>
    {% endblock %}
</div>