{
  "name": "mophidian",
  "version": "1.0.0",
  "description": "Documentation website for the pythonic SSG Mophidian",
  "main": "index.js",
  "repository": "https://github.com/Tired-Fox/Mophidian",
  "author": "Tired Fox (Zachary Boehm) <zboehm104@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.7",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.1"
  },
  "scripts": {
    "css": "sass pages/:static/ styles/:static/css/ --no-source-map",
    "css:watch": "sass --watch pages/:static/ styles/:static/css/ --no-source-map",
    "css:compress": "sass --style=compressed pages/:static/ styles/:static/css/ --no-source-map",
    "css:watch:compress": "sass --watch --style=compressed pages/:static/ styles/:static/css/ --no-source-map",
    "tailwind": "tailwindcss -i ./tailwind.css -o ./static/css/tailwind.min.css",
    "tailwind:mini": "tailwindcss -i ./tailwind.css -o ./static/css/tailwind.min.css --minify",
    "tailwind:watch": "tailwindcss -i ./tailwind.css -o ./static/css/tailwind.min.css --minify --watch",
    "tailwind:watch:mini": "tailwindcss -i ./tailwind.css -o ./static/css/tailwind.min.css --minify --watch --minify"
  }
}